{
  "name": "react-wrench",
  "version": "0.5.1",
  "description": "Utilities for React web applications. :wrench:",
  "repository": "git@github.com:oreqizer/react-wrench.git",
  "author": "oreqizer <oreqizer@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn storybook",
    "build": "yarn typegen && yarn babel",
    "typegen": "yarn tsc && rimraf lib/**/*.js",
    "babel": "babel src -d lib --extensions '.ts,.tsx' --ignore '**/__tests__/*'",
    "test": "jest --passWithNoTests",
    "types": "tsc --noEmit",
    "lint": "eslint './**/*.{js,ts,tsx,js.flow}'",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "rimraf docs/storybook && build-storybook -o docs/storybook",
    "docgen": "node etc/docgen",
    "docgen:check": "node etc/docgen/check",
    "docs": "run-p docgen storybook:build",
    "ci": "run-p test types lint docgen:check",
    "prepare": "rimraf lib && run-p test lint types && yarn build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version": "run-p docgen changelog && git add CHANGELOG.md docs/**"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "yarn ci"
    }
  },
  "resolutions": {},
  "peerDependencies": {
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "@types/react-relay": "^6.0.1",
    "@types/relay-runtime": "^6.0.7",
    "js-cookie": "^2.2.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-relay": "^6.0.0",
    "relay-runtime": "^6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-react-constant-elements": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@storybook/addon-actions": "5.2.5",
    "@storybook/addon-knobs": "^5.2.5",
    "@storybook/addon-links": "5.2.5",
    "@storybook/addons": "5.2.5",
    "@storybook/react": "5.2.5",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.19",
    "@types/js-cookie": "^2.2.4",
    "@types/node": "^12.11.2",
    "@types/ramda": "^0.26.33",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "@types/react-relay": "^6.0.1",
    "@types/relay-runtime": "^6.0.7",
    "@types/storybook__react": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "conventional-changelog-cli": "^2.0.25",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "fs-extra": "^8.1.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "js-cookie": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "raf": "^3.4.1",
    "ramda": "^0.26.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-relay": "^6.0.0",
    "relay-runtime": "^6.0.0",
    "rimraf": "^3.0.0",
    "typescript": "^3.6.4"
  }
}
